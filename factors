#!/usr/bin/env bash

factorize_number() {
    num=$1
    factors=$(python3 -c "from sympy import factorint; print(factorint($num))")
    echo "$num=$factors"
}

if [ $# -ne 1 ]; then
    echo 'Usage: factors <file>'
    exit 1
else
    while read i; do
        factorize_number "$i"
    done < "$1"
fi

